{
    "name": "xsolve-pl/associate",
    "description": "This library allows to collect objects and values through associations and provides some entity fetching optimizations for Doctrine ORM to address N+1 queries problem.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mariusz BÄ…k",
            "email": "mariusz.alef.bak@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/property-access": "^3.0|^4.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4,>=2.4.5",
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.9.2",
        "symfony/var-dumper": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "suggest": {
        "doctrine/orm": "^2.4,>=2.4.5",
        "webonyx/graphql-php": "^0.0,>=0.9"
    },
    "autoload": {
        "psr-4": {
            "Xsolve\\Associate\\": "lib",
            "Xsolve\\LegacyAssociate\\": "legacyLib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xsolve\\AssociateTests\\": "tests",
            "Xsolve\\LegacyAssociateTests\\": "legacyTests"
        }
    }
}
